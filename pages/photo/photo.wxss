.photo-warp{
  padding-top: 40rpx;
}
.photo-warp ul li{
    float: left;
    margin-bottom:20rpx;
    margin-left:20rpx;
    width:200rpx;
    height:200rpx;
    text-align: center
}
.photo-warp ul li image{
    width:200rpx;
    height:200rpx;
}

.add-warp{
    float: left;
    position: relative;
    margin-left:20rpx;
    margin-bottom:20rpx;
    width: 200rpx;
    height: 200rpx;
    border: 1px solid #D9D9D9;
}

.add-warp:before, .add-warp:after {
    content: " ";
    position: absolute;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    background-color: #D9D9D9;
}
.add-warp:before{
    width: 2px;
    height: 40px;
}
.add-warp:after{
    width: 40px;
    height: 2px;
}


