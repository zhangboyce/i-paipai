.photo-containder{
  margin-top: 20rpx;
}

.total {
  font-size: 26rpx;
  margin-right:28rpx;
  text-align: right;
  margin-top: 20rpx;
}

.date-text{
  font-size: 26rpx;
  margin-left:28rpx;
  padding-bottom:10rpx;
}

.photo-list {
  display: flex;
  flex-direction: row;
  flex-wrap:wrap;
  width: 98%;
  margin:10rpx auto 0;
}

.img-container{
  float: left;
  width: 33.3%;
  display: flex;
  justify-content:center;
  align-items: center;
}

.photo-list image {
  height: 200rpx;
  margin-bottom: 20rpx;
  width: 100%;
  padding: 0 2% 0 2%;
}

.no-content {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.no-content image {
  height: 128rpx;
  width: 128rpx;
  margin-top: 160rpx;
}

.no-content text {
  font-size: 28rpx;
  color: #b5acaa;
  margin-top: 10rpx;
}